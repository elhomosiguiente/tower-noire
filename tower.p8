pico-8 cartridge // http://www.pico-8.com
version 4
__lua__

function _init()
	--init game variables
	gm = {}
	gm.wait = false
	gm.input_wait = false
	gm.log = {"thanks.", "sorry. thanks."}
	pal(6,0)

	--init player variables and functions
	p = {}
	p.hp = 6
	p.x = 16
	p.y = 8
	p.steps = 0
	p.sprites = {0}
	p.image = 0
	
	function gm.draw()
		mapdraw(0,0,0,0,16,16) --the test map
		spr(31+((p.steps % 2)*16), 8, 8) --the bloodfiresoul
		spr(32+((p.steps % 2)*16), 32, 64) --the slime
		spr(33+((p.steps % 2)*16), 40, 80) --the rat
		print(gm.log[count(gm.log)-1],4,115,1)
		print(gm.log[count(gm.log)-1],4,114,5)
		print(gm.log[count(gm.log)],4,123,1)
		print(gm.log[count(gm.log)],4,122,13)
	end
	
	function p.update()
		p.move()
		p.draw()
	end
	
	function p.step(x,y,img)
		p.x += x
		p.y += y
		p.steps += 1
		p.image = img+(p.steps%2)
		sfx(1+(p.steps%2))
	end
	
	function p.move()
		if btnp(0) then
			p.step(-8,0,0)
		end
		if btnp(1) then
			p.step(8,0,2)
		end
		if btnp(2) then
			p.step(0,-8,4)
		end
		if btnp(3) then
			p.step(0,8,6)
		end
		if btnp(4) then
			sfx(3)
			add(gm.log,"you approach the ladder...")
		end
	end
	
	function p.draw()
		spr(p.image,p.x,p.y)
	end
end

function _update()
end

function _draw()
	cls()
	gm.draw()
	p.update()
end


























