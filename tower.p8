pico-8 cartridge // http://www.pico-8.com
version 4
__lua__

function _init()
	--init game variables
	gm = {}
	gm.wait = false
	gm.input_wait = false
	gm.log = {"welcome to tower noire.", "sorry. thanks."}
	gm.crs = ">"
	gm.prompted = false
	pal(6,0)

	--init player variables and functions
	p = {}
	p.can = true
	p.hp = 16
	p.maxhp = 16
	p.hand = "none"
	p.body = "tunic"
	p.legs = "none"
	p.x = 16
	p.y = 8
	p.steps = 0
	p.sprites = {0}
	p.image = 0
	p.actions = {{gm.crs,"wear"},{"","throw"},{"","dip"},{"","eat"},{"","drop"},{"","exit"}}
	
	function gm.mapdraw()
		mapdraw(0,0,0,0,16,16) --the test map
		spr(31+((p.steps % 2)*16), 8, 8) --the bloodfiresoul
		--spr(32+((p.steps % 2)*16), 32, 64) --the slime
		--spr(33+((p.steps % 2)*16), 40, 80) --the rat
	end
	
	function gm.draw()
		--draw log
		print(gm.log[count(gm.log)-1],4,115,1)
		print(gm.log[count(gm.log)-1],4,114,5)
		print(gm.log[count(gm.log)],4,123,1)
		print(gm.log[count(gm.log)],4,122,13)
		--draw side hud
		spr(63,98,0)
		print(p.hp .. "/" .. p.maxhp,108,1,7)
		spr(45,98,8)
		if p.hand == "none" then print(p.hand,108,9,5) else print(p.hand,108,9,13) end
		spr(44,98,16)
		if p.body == "none" then print(p.body,108,17,5) else print(p.body,108,17,13) end
		spr(46,98,24)
		if p.legs == "none" then print(p.legs,108,25,5) else print(p.legs,108,25,13) end
		
		if gm.prompted == true then
			gm.prompt()
		end
	end
	
	function gm.debugdraw()
		if btn(0) then rectfill(108,108,112,112,7) else rect(108,108,112,112,5) end
		if btn(1) then rectfill(120,108,124,112,7) else rect(120,108,124,112,5) end
		if btn(2) then rectfill(114,102,118,106,7) else rect(114,102,118,106,5) end
		if btn(3) then rectfill(114,108,118,112,7) else rect(114,108,118,112,5) end
		if btn(4) then rectfill(108,114,112,118,7) else rect(108,114,112,118,5) end
		if btn(5) then rectfill(114,114,118,118,7) else rect(114,114,118,118,5) end
	end
	
	function gm.next()
		--go to next room
	end
	
	function gm.prompt()
		p.can = false
		rect(16,32,111,79,7)
		rectfill(17,33,110,78,0)
		line(16,80,111,80,1)
		print("what would you like",24,40,7)
		print("to do with that?",24,48,7)
		for i=1,6 do
			if i <= 3 then
				print(p.actions[i][1]..p.actions[i][2],(24+(i-1)*28),58,7)
			else
				print(p.actions[i][1]..p.actions[i][2],(24+(i-4)*28),66,7)
			end
		end
		if btnp(0) then
			for i=1,6 do
				if p.actions[i][1] == gm.crs then
					p.actions[i][1] = ""
					if i==1 then
						p.actions[6][1] = gm.crs
						break
					else
						p.actions[i-1][1] = gm.crs
						break
					end
				end
			end		
		end
		if btnp(1) then
			for i=1,6 do
				if p.actions[i][1] == gm.crs then
					p.actions[i][1] = ""
					if i==6 then
						p.actions[1][1] = gm.crs
						break
					else
						p.actions[i+1][1] = gm.crs
						break
					end
				end
			end	
		end
		if btnp(4) then
			for i=1,6 do
				if p.actions[i][1] == gm.crs then
					if i==6 then
						gm.prompted = false
						p.can = true
						flip()
					end
					--fill in the rest of the behaviours here
				end
			end
		end	
	end
	
	function p.update()
		p.move()
		p.draw()
	end
	
	function p.step(x,y,img)
		to = mget((p.x+x)/8,(p.y+y)/8)
		if to != 60 and to != 61 and to != 65 then
			p.x += x
			p.y += y
			p.steps += 1
			p.image = img+(p.steps%2)
			sfx(1+(p.steps%2))
		end
	end
	
	function p.move()
		if p.can == true then
			if btnp(0) then
				p.step(-8,0,0)
			end
			if btnp(1) then
				p.step(8,0,2)
			end
			if btnp(2) then
				p.step(0,-8,4)
			end
			if btnp(3) then
				p.step(0,8,6)
			end
			if btnp(4) then
				gm.prompted = true
				flip()
			end
			if btnp(5) then
				--sfx(3)
			end
		end
	end
	
	function p.draw()
		spr(p.image,p.x,p.y)
	end
end

function _update()
end

function _draw()
	cls()
	gm.mapdraw()
	p.update()
	gm.draw()
	gm.debugdraw()
end












































